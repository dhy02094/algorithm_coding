   "source": [
    "def solution(nums):\n",
    "    answer = 0\n",
    "    a = set(nums)\n",
    "    if len(nums)//2 < len(a):\n",
    "        answer = len(nums)//2\n",
    "    else :\n",
    "        answer = len(a)\n",
    "    return answer"
   ]
  }
