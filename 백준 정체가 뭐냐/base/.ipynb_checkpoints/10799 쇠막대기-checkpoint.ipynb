    "a = list(input())\n",
    "stack = []\n",
    "cnt = 0\n",
    "for i in range(len(a)):\n",
    "    if a[i]=='(':\n",
    "        stack.append(a[i])\n",
    "    else:\n",
    "        stack.pop()\n",
    "        if a[i-1]=='(':\n",
    "            cnt += len(stack)\n",
    "        else :\n",
    "            cnt += 1\n",
    "print(cnt)"
   ]
