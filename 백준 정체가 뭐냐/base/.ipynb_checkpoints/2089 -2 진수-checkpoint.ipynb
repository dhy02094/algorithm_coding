    "n = int(input())\n",
    "ans = ''\n",
    "if n == 0 :\n",
    "    print(0)\n",
    "    exit()\n",
    "while n != 0:\n",
    "    if n%-2:\n",
    "        n = n//-2 + 1\n",
    "        ans = '1' + ans\n",
    "    else :\n",
    "        n//=-2\n",
    "        ans ='0' + ans\n",
    "print(int(ans))"
   ]
