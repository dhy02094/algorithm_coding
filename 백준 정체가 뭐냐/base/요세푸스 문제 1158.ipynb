   "source": [
    "from collections import deque\n",
    "\n",
    "n, k = map(int, input().split())\n",
    "dq = deque([i for i in range(1,n+1)])\n",
    "rlt = []\n",
    "while dq:\n",
    "    for _ in range(k-1):\n",
    "        dq.rotate(-1)\n",
    "    rlt.append(dq.popleft())\n",
    "\n",
    "print('<',', '.join(str(i) for i in rlt)[:], \">\", sep=\"\")\n",
    "    \n",
    "        "
   ]
  }
