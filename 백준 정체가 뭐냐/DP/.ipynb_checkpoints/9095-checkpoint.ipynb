    "n = int(input())\n",
    "dp = [0]*11\n",
    "dp[1] = 1\n",
    "dp[2] = 2\n",
    "dp[3] = 4\n",
    "for _ in range(n):\n",
    "    a = int(input())\n",
    "    for i in range(4,a+1):\n",
    "        dp[i] = dp[i-3]+dp[i-2]+dp[i-1]\n",
    "    print(dp[a])"
