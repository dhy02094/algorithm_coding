   "source": [
    "n = int(input())\n",
    "\n",
    "dp = [[0]*10 for _ in range(n+1)]\n",
    "\n",
    "for i in range(10):\n",
    "    dp[1][i] = 10-i\n",
    "\n",
    "for i in range(2,n+1):\n",
    "    for j in range(10):\n",
    "        dp[i][j] = sum(dp[i-1][j:])\n",
    "print(dp[n][0]%10007)"
   ]
